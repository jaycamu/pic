{
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "-theme-faq",
        "include-codeblock",
        "ace",
        "chapter-fold",
        "anchor-navigation-ex-toc",
	"back-to-top-button",
	"image-captions",
	"cuav-chapters",
	"page-numbering",
	"hide-element"
    ],
    "pluginsConfig": {
	"theme-default": {
	    "showLevel": true
	},
	"image-captions": {
		"align": "center",
		"attributes": { "width": "300" },
		"caption": " 图片 _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
	},
	"page-numbering": {
            "skipReadme": false,
            "chapterFormat": "Chapter #chapno#&nbsp;&nbsp;&nbsp;#title# ||| #chapno#&nbsp;&nbsp;&nbsp;#title#",
            "forceMultipleParts": false
        },
	"pluginsConfig": {
		"include-codeblock": {
			"template": "ace",
			"unindent": true,
			"theme": "monokai"
		}
	},
	"hide-element": {
            "elements": [".gitbook-link"]
        }
    }
}
